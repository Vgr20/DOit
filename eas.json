{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "CLIENT_API": "@env.CLIENT_API",
        "APP_API": "@env.APP_API"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "CLIENT_API": "@env.CLIENT_API",
        "APP_API": "@env.APP_API"
      }
    },
    "production": {
      "env": {
        "CLIENT_API": "@env.CLIENT_API",
        "APP_API": "@env.APP_API"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
